function openNoticePopup () {
    jQuery('#goodsRegistNoticePopup').show();
}

function closeNoticePopup () {
    jQuery('#goodsRegistNoticePopup').hide();
}

function calendarPrint(elem, date) {
    if (jQuery(elem).hasClass('calendar_date')) {
        jQuery(elem).val(date.replace(/[^(0-9)]/gi, ''));

    } else if (jQuery(elem).hasClass('calendar_diff_day')) {
        var sdt  = new Date();
        var edt  = new Date(date);
        var diff = Math.ceil((edt.getTime() - sdt.getTime()) / (1000 * 3600 * 24));

        jQuery(elem).val(diff);
    }
}

/* floating buttons show/hide event */
function showScrollBtns(upStatus, downStatus) {
    if (upStatus) {
        jQuery('.scrollUpBtn').show();
    } else {
        jQuery('.scrollUpBtn').hide();
    }

    if (downStatus) {
        jQuery('.scrollDownBtn').show();
    } else {
        jQuery('.scrollDownBtn').hide();
    }
}

/** 배송 약정 */
function orderDayActive(orderDaysArr) {
    for (var k in orderDaysArr) {
        jQuery('[name="' + orderDaysArr[k] + '"]').attr('disabled', !jQuery('[name="' + k + '"]').is(':checked'))
    }
}

