// 전역 ajax progress
(function($) {
    $(document)
        .ajaxSend(function (event, request, settings) {
            $('#sellerhubAjaxLoader').show();
        })
        .ajaxComplete(function (event) {
            $('#sellerhubAjaxLoader').hide();
        });
})(jQuery);
</script>


