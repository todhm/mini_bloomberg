    <script type="text/javascript">
        function fbrand_goodsnm() {
            var brandnm = '';

            if (jQuery("#brand_goodsnm").is(":checked")) {
                if (jQuery("select[name=brandno] option:selected").val() != '' && jQuery("select[name=brandno] option:selected").val() != undefined) {
                    if (jQuery("select[name=brandno] option:selected").data('subname') != '') {
                        brandnm = jQuery("select[name=brandno] option:selected").data('subname');
                    } else {
                        brandnm = jQuery("select[name=brandno] option:selected").data('brandnm');
                    }

                    if (jQuery('input[name="goodsnm"]').val().indexOf(brandnm) === -1) {
                        jQuery('input[name="goodsnm"]').val("[" + brandnm + "] " + jQuery('input[name="goodsnm"]').val());
                    }

                    checkByte();
                }
            }

            if (jQuery('select[name="brandno"]').val()) {
                jQuery('input[name="goodsnm"]').removeAttr('disabled');
            } else {
                jQuery('input[name="goodsnm"]').attr('disabled', true);
            }
        }

        function check_brand() {
            if (typeof jQuery('input[name="goodsnm"]').attr('disabled') !== 'undefined' && !jQuery('select[name="brandno"]').val()) {
                alert('브랜드를 먼저 선택해주세요.');
                jQuery('select[name="brandno"]').focus();
            }
        }
    </script>

