    <script src="/vendor/ckeditor/ckeditor/ckeditor.js"></script>
    <script type="text/javascript">
        CKEDITOR.replace('longdesc', {
            contentsCss: '/shop/lib/ckeditor/ckeditor.css',
            height: 600,
            imageUploadUrl: '/shop/lib/ckeditor/indb.php?type=drag',
            filebrowserImageUploadUrl: '/shop/lib/ckeditor/indb.php?type=upload',
            filebrowserImageUploadMethod: 'form',
            on: {
                instanceReady: function (event) {
                    event.editor.dataProcessor.dataFilter.addRules({
                        elements : {
                            img: function(element) {
                                element.addClass('ckeditor-upload-img');
                            }
                        }
                    });
                },
                afterInsertHtml: function (event) {
                    if (typeof event.data.intoRange != 'undefined') {
                        var list = Array.prototype.slice.call(event.data.intoRange.endContainer.$.childNodes, 0).filter(function (node) {
                            return jQuery(node).hasClass('ckeditor-upload-img') && jQuery(node).next().prop('tagName') !== 'BR';
                        });

                        jQuery.each(list, function (index, node) {
                            jQuery(node).after('<br>');
                        });
                    }
                },
                fileUploadRequest: function (event) {
                    var fileLoader = event.data.fileLoader;

                    var file = fileLoader.file;
                    var type = fileLoader.uploadUrl.split('?')[1];

                    if (file.size > 5000000) {
                        var widgets = event.sender.widgets.instances;

                        jQuery.each(widgets, function(index, widget) {
                            jQuery(widget.wrapper.$).remove();
                        });

                        var msg = '이미지파일 용량이 5MB 이상인 경우 등록이 불가합니다.\n용량을 축소하여 재 등록하시거나, 이미지가 길 경우 나누어 등록해 주시길 바랍니다.';

                        if (type.indexOf('drag') > -1) {
                            alert(msg);
                        } else if (type.indexOf('upload') > -1) {
                            CKEDITOR.tools.callFunction(0, '', msg);
                        }

                        event.cancel();
                    }
                }
            }
        });
    </script>

