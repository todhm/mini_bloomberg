    }
    
    function loadSellerTalk () {
        var w = window;
        if (w.ChannelIO) {
            return (window.console.error || window.console.log || function(){})('ChannelIO script included twice.');
        }
        var d = window.document;
        var ch = function() {
            ch.c(arguments);
        };
        ch.q = [];
        ch.c = function(args) {
            ch.q.push(args);
        };
        w.ChannelIO = ch;
        function l() {
            if (w.ChannelIOInitialized) {
                return;
            }
            w.ChannelIOInitialized = true;
            var s = document.createElement('script');
            s.type = 'text/javascript';
            s.async = true;
            s.src = '//cdn.channel.io/plugin/ch-plugin-web.js';
            s.charset = 'UTF-8';
            var x = document.getElementsByTagName('script')[0];
            x.parentNode.insertBefore(s, x);
        }
        if (document.readyState === 'complete') {
            l();
        } else if (window.attachEvent) {
            window.attachEvent('onload', l);
        } else {
            window.addEventListener('DOMContentLoaded', l, false);
            window.addEventListener('load', l, false);
        }
    }

    function bootSellerTalk (settings) {
        window.ChannelIO('onBoot', function (user) {
            if (user.memberId != settings.memberId) {
                setCookie('ch-session-'+user.channelId);
            }
        });

        window.ChannelIO('boot', settings);
    }

    loadSellerTalk();
    bootSellerTalk(settings);
    
    function informMenu(){
        var btnInform = document.getElementById("btnInform");

        if (btnInform.innerHTML == "닫기"){
            btnInform.innerHTML = "펼치기";
            btnInform.parentElement.classList.toggle('open');
            sessionStorage.setItem('sellerhub_show_floating_bar', 0);
        } else {
            btnInform.innerHTML = "닫기";
            btnInform.parentElement.classList.toggle('open');
            sessionStorage.setItem('sellerhub_show_floating_bar', 1);
        };
    }

    (function($) {
        if (sessionStorage.getItem('sellerhub_show_helper')*1) {
            jQuery(".open-buttons").show();
            jQuery(".close-buttons").hide();
        } else {
            jQuery(".open-buttons").hide();
            jQuery(".close-buttons").show();
        }

        if ( sessionStorage.getItem('sellerhub_show_floating_bar') == null || sessionStorage.getItem('sellerhub_show_floating_bar') == 1 ) {
            informMenu();
        }

        jQuery(".btn_top").click(function() {
            jQuery('html, body').animate({
                scrollTop : 0
            }, 400);
            return false;
        });

        // 무료이용기간
        jQuery(".btn-freeperiod").on("click", function () {
            jQuery(".layer_freeperiod").toggleClass("show");
        });
        jQuery(".layer_freeperiod .btn_closed").on("click", function () {
            jQuery(".layer_freeperiod").removeClass("show");
        });

        jQuery(".side-close").on("click", function () {
            jQuery(".open-buttons").hide();
            jQuery(".close-buttons").show();
            sessionStorage.setItem('sellerhub_show_helper', 0);
        });

        jQuery(".chk-icon").on("click", function () {
            jQuery(".open-buttons").show();
            jQuery(".close-buttons").hide();
            sessionStorage.setItem('sellerhub_show_helper', 1);
        });
    })(jQuery);
</script>
