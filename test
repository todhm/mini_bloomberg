    <script type="text/javascript">
        //연동 옵션..
        
        function gotoList(link) {
            opener.location.href = link;
        }

        function fnSetImageAttachForm() {
            var m, obj = document.fm.image_attach_method;

            for (var i = 0; i < obj.length; i++) {
                if (obj[i].checked)
                    var m = obj[i].value;
            }

            if (m == 'file') {
                jQuery('#image_attach_method_upload_wrap, .image_attach_method_upload_wrap').show();
                jQuery("#image_attach_method_link_wrap, .image_attach_method_link_wrap").hide();
            } else {
                jQuery("#image_attach_method_upload_wrap, .image_attach_method_upload_wrap").hide();
                jQuery("#image_attach_method_link_wrap, .image_attach_method_link_wrap").show();
            }
        }

        function fnSetDescAttachForm() {

            var m, obj = document.fm.longdesc_auto;

            for (var i = 0; i < obj.length; i++) {
                if (obj[i].checked)
                    var m = obj[i].value;
            }

            if (m == 'Y') {
                document.getElementById('longdesc_container').style.display = 'none';
            } else {
                document.getElementById('longdesc_container').style.display = 'block';
            }

        }

        function applydopt() {
            var obj = document.getElementById('dopt');
            var k = obj.selectedIndex;

            if (obj[k].value) {
                //2018-10-22 ssi 옵션 표 초기화
                var newHtml = '<tr class="com_data_1"><td class="bt1 br1 bl1 bg_f9"></td><td class="bt1 br1 bl1 bg_f9" align="center">쇼핑몰판매가</td>	<td class="bt1 br1 bb1 bg_f9" align="center">소비자정상가</td>	<td class="bt1 br1 bb1 bg_f9 none" align="center">공급가</td></tr>';
                jQuery("#tbOption tbody").html(newHtml);

                ifrmHidden.location.href = "popup.dopt_register.php?mode=dopt_apply&sno=" + obj[k].value;
            }
        }

        /* 옵션 부분 삭제 */
        function delopt1part(rid) {
            var obj = document.getElementById(rid);
            var tbOption = document.getElementById('tbOption');
            var idx = obj.rowIndex;
            if (tbOption.rows.length > 3) tbOption.deleteRow(obj.rowIndex);
//	delopt1part_fashion(idx);
        }

        function delopt2part(cid) {
            var obj = document.getElementById(cid);
            var delCellIndex = document.getElementById(cid).cellIndex;
            var tbOption = document.getElementById('tbOption');
            var idx = obj.cellIndex;

            if (tbOption.rows[1].cells.length < 6) return;

            for (i = 0; i < tbOption.rows.length; i++) {
                tbOption.rows[i].deleteCell(delCellIndex);
            }

            delopt2part_fashion(idx);

        }

        /*** 폼체크 ***/
        function chkForm2(obj, isPreview) {

            // 상품 쇼핑몰판매가 가격 옵션과 상이할 경우 체크.
            var price_flag = false;

            var _price = jQuery("input[name='price']").val();
            var inputs_price = jQuery("input[name^='option_price']");

            for(var i = 0; i < inputs_price.length; i++) {
                if (inputs_price[i].value == _price) continue;
                price_flag = true;
            }

            // 상품명 길이 체크 -- 151214 이지연
            if (byteFlag != 1) {
                alert('상품명은 최대 55Byte까지 작성 가능합니다.');
                jQuery("input[name=goodsnm]").focus();
                return false;
            }

            if (jQuery("select[name=brandno] option:selected").val() == '') {
                alert("브랜드는 필수 선택사항입니다.");
                jQuery("select[name=brandno]").focus();
                return false;
            }

            if (typeof (obj['category[]']) == "undefined") {
                if (document.getElementsByName("cate[]")[0].value) exec_add();
                else {
                    alert("카테고리를 등록해주세요");
                    document.getElementsByName("cate[]")[0].focus();
                    return false;
                }
            }
            if (!chkTitle()) {
                alert('항목명은 중복될 수 없습니다.');
                return false;
            }

            // 주문제작, 예약발송, 해외배송 옵션 체크
            var chkDelivery = chkDeliveryOption();
            if (chkDelivery) {
                alert('['+chkDelivery+'] 누락된 값이 존재합니다.');
                return false;
            }

            if (!chkOption()) return false;
            if (!chkForm(obj)) return false;

            /*준수판매가*/
			
            if (!chkImg()) {
                alert('이미지는 최소 1개 이상 등록해야 합니다.');
                return false;
            }

            /*new option form check*/
            if (!shAdminGoodsForm.validate()) return false;

            /*친환경 인증 첨부*/
            if (!shAdminGoodsForm.goodsCertAttach.validateMemoSize()) return false;

            // 마더케어 공급사의 경우 상품 설명 설정 한번 더 확인하게끔 안내 - 160406 이지연
            var providersno = "24270";
            if (providersno == "423") {
                if (!confirm("상품 설명 탭이 자동입력에 체크된 경우는 상하단 스킨 이미지와 상품 상세이미지를 제외한 나머지 이미지들이 자동 삭제됩니다.\n상품 설명 설정을 확인하셨습니까?")) {
                    return false;
                }
            }

            // 상품정보고시는 필수사항입니다.
            if ( !jQuery("select[name=gosi_goodsRequired]").val() ) {
                alert("상품정보고시를 등록해주세요.");
                jQuery("select[name=gosi_goodsRequired]").focus();
                return false;
            }

            
            try {
                // 관련 상품 정보
                nsRelatedGoods.make();
            } catch (e) {
                console.log(e.description);
            }

			/*
			상품명 금칙어 체크
			*/
			var word_flag = true;
			var cate_arr = [];
			jQuery('input[name="category[]"]').each(function(i) {
				cate_arr.push(jQuery(this).val());
			});

			var forbidden_params = {
				"_cls": "check_forbidden_goods_nm",
				"providerSno": '24270',
				"goodsnm": jQuery("input[name='goodsnm']").val(),
				"longdesc": CKEDITOR.instances.longdesc.getData(),
				"keyword": jQuery('input[name="keyword"]').val(),
				"category": cate_arr,
			};

			jQuery.ajax({
				url: "/shop/admin/sh_ajax_handle.php",
				type: "POST",
				async: false,
				data: forbidden_params,
				dataType: "json",
				success: function (resp) {
					if (resp['bool'] == 'false'){
						alert(resp['msg']);
						word_flag = false;
					}
				}
			});

			if (!word_flag) return word_flag;

            var params = {
                "_cls": "check_goods_nm",
                "goodsno": "",
                "sno": "24270",
                "goodsnm": jQuery("input[name='goodsnm']").val()
            };

            
            jQuery.ajax({
                url: "/shop/admin/sh_ajax_handle.php",
                type: "POST",
                data: params,
                dataType: "json",
                success: function (resp) {
                    // console.log(resp);
                    if (resp.result == 'exist') {
                        alert("동일한 상품명이 존재합니다.\n상품명을 변경해 주시기 바랍니다.");

                    } else {
                        //상품 쇼핑몰판매가와 옵션별 쇼핑몰판매가가 다를 경우 안내문구
                        if (price_flag) {
                            alert("- 옵션가는 쇼핑몰판매가와 동일한 가격으로만 등록\n가능합니다\n- 각 옵션 별 쇼핑몰판매가가 다른 경우 제휴몰 별로\n쇼핑몰판매가 등록 오류 및 옵션이 누락되는 등의\n문제가 있을 수 있으니 가격이 다른 옵션은 다른\n상품으로 분리하여 등록하시길 권장합니다.");
                        }

                        if (isPreview) {
                            var preview = window.open('/shop/goods/goods_preview.php', 'preview');

                            jQuery('input[name="attach_imgs[]"]').remove();
                            jQuery.each(attach_imgs, function(index, img) {
                                if (typeof img != 'undefined') {
                                    var attach_imgs = '<input type="hidden" name="attach_imgs[]" id="attach_imgs_'+index+'" value="'+img+'" />';
                                    jQuery(document.fm).append(attach_imgs);
                                }
                            });

                            document.fm.mode.value = 'preview';
                            document.fm.setAttribute('action', '/shop/goods/goods_preview.php');
                            document.fm.setAttribute('target', 'preview');
                            document.fm.submit();

                            document.fm.mode.value = 'register';
                            document.fm.setAttribute('action', 'indb.goods.php');
                            document.fm.removeAttribute('target');

                        } else {
                            jQuery('input[name="attach_imgs[]"]').remove();

                            jQuery("#goods-form").submit();
                        }
                    }
                }
            });

            return false;
        }

        /*** 상품 카테고리 선택 ***/
        var idxCategory;
        var preCurrposSel;

        function cate_mod(obj, el) {
            el.style.background = "#EFF5F9";
            idx = el.rowIndex;
            var objX = document.getElementsByName('category[]');
            var val = objX[idx].value;
            idxCategory = idx;
            if (preCurrposSel && preCurrposSel != el) preCurrposSel.style.background = "#FFFFFF";
            preCurrposSel = el;
            categoryBox_request(obj, val);
        }

        function cate_del(obj) {

            var goodsno_ = '';
            var data = jQuery(obj).parent().parent().find("input[name='category[]']").val();

            jQuery.ajax({
                type: "POST",
                url: "delete_category.php",
                data: {"cate_data": data, "goodsno": goodsno_},
                dataType: "json",
                cache: false,
                success: function (data) {
                    alert(data['msg']);
                    jQuery(obj).parent().parent().remove();
                }
            });

        }

        function exec_mod() {
            if (typeof (idxCategory) == "undefined") {
                alert('수정할 카테고리를 선택해주세요');
                return;
            }
            var ret;
            var str = new Array();
            var obj = document.forms[0]['cate[]'];
            for (i = 0; i < obj.length; i++) {
                if (obj[i].value) {
                    str[str.length] = obj[i][obj[i].selectedIndex].text;
                    ret = obj[i].value;
                }
            }
            if (!ret) return;
            obj = document.getElementsByName('category[]');
            if (obj[idxCategory]) obj[idxCategory].value = ret;
            obj = document.all.currPosition;
            if (obj) {
                if (!(obj.length > 0)) obj = new Array(obj);
                obj[idxCategory].innerHTML = str.join(" > ");
            }
        }

        function exec_add() {
            var ret;
            var str = new Array();
            var obj = document.forms[0]['cate[]'];
            for (i = 0; i < obj.length; i++) {
                if (obj[i].value) {
                    str[str.length] = obj[i][obj[i].selectedIndex].text;
                    ret = obj[i].value;
                }
            }

            if (!ret) {
                alert('카테고리를 선택해주세요');
                return;
            }

            if (ret.length < 12) {
                alert("카테고리는 4차까지 선택해 주셔야 합니다.\n\n적절한 카테고리가 없는 경우 운영팀에 문의해 주세요.");
                return;
            }

            var clone_main = "<div class='in_ps in_1'><span>" + str.join(" > ") + "</span><div class='in_fm'><input type='text' name='category[]' value='" + ret + "' style='display:none;'><input type='text' name='sort[]' value='1599550698' class='certification_info' maxlength='10' style='display:none' style='width:250px;'><a href='javascript:void(0)' onClick='cate_del(this)'><span class='b_x'>x 삭제</span></a></div></div>";
            var clone = jQuery(clone_main).eq(0).clone();
            jQuery(clone).children().find("input[name='category[]']").val(ret);
            jQuery(clone).children().find("input[name='sort[]']").val('1599550698');
            jQuery(clone).children('span').eq(0).text(str.join(" > "));

            var goodsno_ = '';
            var data = jQuery(clone).children().find("input[name='category[]']").val();

            if (goodsno_.length > 0) {
                jQuery.ajax({
                    type: "POST",
                    url: "insert_category.php",
                    data: {"mode": "check", "cate_data": data, "goodsno": goodsno_},
                    dataType: "json",
                    cache: false,
                    success: function (data_) {
                        if (data_['bool'] == 'false') {
                            alert(data_['msg']);
                        } else {
                            jQuery.ajax({
                                type: "POST",
                                url: "insert_category.php",
                                data: {"mode": "insert", "cate_data": data, "goodsno": goodsno_},
                                dataType: "json",
                                cache: false,
                                success: function (data__) {
                                    alert(data__['msg']);
                                    jQuery("#objCategory").append(clone);
                                }
                            });
                        }
                    }
                });

            } else {
                jQuery("#objCategory").append(clone);
            }
        }

        /*** 상품 가격/재고 ***/
        function addopt1() {
            var name;
            var fm = document.forms[0];
            var Rcnt = jQuery("#tbOption > tbody").find('tr').length;
            var rowId = "trid_" + Rcnt;
            var td_len = jQuery("#tbOption > tbody").find('tr').eq(0).find('td').length;

            var html = '<tr id="' + rowId + '" class="com_data_2">';
            html += '	<td class="bt1 br1 bl1 bb1 bg_f9"></td>';
            for (i = 1; i < td_len; i++) {
                html += '	<td class="bt1 br1 bb1"></td>';
            }
            html += '</tr>';

            jQuery("#tbOption tbody:last").append(html);
            idx = jQuery("#tbOption tbody tr:last").index();
            jQuery("#tbOption > tbody > tr").each(function (index, value) {
                if (index != 0 && idx == index) {

                    var id = jQuery(this).attr("id");

                    for (i = 0; i < td_len; i++) {
                        switch (i) {
                            case 0:
                                jQuery(value).find('td').eq(0).html("<input type=text name=opt1[] value='옵션명1' required label='1차옵션명' ondblclick=\"delopt1part('" + id + "')\" onclick='chkOptName(this)' onblur=\"chkOptNameOver(this)\">");
                                break;
                            case 1:
                                jQuery(value).find('td').eq(1).html("<input type=text name=option[price][]  value='" + fm.price.value + "' ><input type='hidden' name='old_price[]' value='" + fm.price.value + "'>");
                                break;
                            case 2:
                                jQuery(value).find('td').eq(2).html("<input type=text name=option[consumer][] value='" + fm.consumer.value + "' ><input type='hidden' name='old_consumer[]' value='" + fm.consumer.value + "'>");
                                break;
                            case 3:
                                jQuery(value).find('td').eq(3).html("<input type=text name=option[supply][] value='" + fm.supply.value + "' >");
                                break;
                            default:
                                jQuery(value).find('td').eq(i).html("<input type=text name=option[stock][] value='재고수량 입력 (예. 99)' onclick='chkOptName(this)' onblur=\"chkOptNameOver(this)\"><input type=hidden name=option[optno][]>");
                                break;
                        }
                    }
                }
            });

//	addopt1_fashion();
        }

        function addopt2() {
            var name;
            var tbOption = document.getElementById('tbOption');

            if (jQuery("#tbOption > tbody").find('tr').length < 2) {
                alert('1차옵션을 먼저 추가해주세요');
                return;
            }

            var Ccnt = jQuery("#tbOption > tbody").find('tr:first td').length;

            var tdid = "tdid_" + Ccnt;
            var innerString = "";

            jQuery("#tbOption tbody tr").each(function (index, value) {
                var id = '';
                if (!index) id = tdid;

                var html = '';

                if (index > 0) {
                    html += "<td class='bt1 br1 bb1'><input type=text name=option[stock][]  value='재고수량 입력 (예. 99)' onclick='chkOptName(this)' onblur=\"chkOptNameOver(this)\"><input type=hidden name=option[optno][]></td>";
                } else {
                    html += "<td id='" + id + "' class='bt1 br1 bb1 bg_f9'><input type=text name=opt2[] value='옵션명2' required label='2차옵션명' ondblclick=\"delopt2part('" + tdid + "')\" onclick='chkOptName(this)' onblur=\"chkOptNameOver(this)\"></td>";
                }

                jQuery("#tbOption tbody tr:eq(" + index + ")").append(html);
            });


            var colspan_r = jQuery("#tbOption tbody tr:eq(0) td").length;
            jQuery("#tbOption tfoot tr:eq(0) td:eq(0)").attr('colspan', colspan_r);
//	addopt2_fashion();

            init_file();
        }

        function delopt1() {
            var tbOption = document.getElementById('tbOption');
            var Rcnt = tbOption.rows.length;
            if (Rcnt > 3) tbOption.deleteRow(Rcnt - 2);
            delopt1_fashion();
        }

        function delopt2() {
            var tbOption = document.getElementById('tbOption');
            var Rcnt = tbOption.rows.length;
            var Ccnt = tbOption.rows[1].cells.length;
            if (Ccnt < 6) return;
            for (i = 0; i < Rcnt; i++) {
                tbOption.rows[i].deleteCell(Ccnt - 1);
            }
            delopt2_fashion();
        }

        /*** 추가옵션 ***/
        function add_addopt() {
            var rowindex = jQuery("#tbAdd").parents('table').find('tbody > tr').length - 1;

            var html = '<tr class="com_data_1">';
            html += '		<td class="bt1 bb1 bl1 br1">';
            html += '			<input type="text"name="addoptnm[]" class="i60">';
            html += '			<button type="button" onclick="add_subadd(this)" class="op_bt op_bt2">항목 추가</button>';
            html += '		</td>';
            html += '		<td colspan="2" class="bt1 bb1 bl1 br1 ta_center">';
            html += '			<div style="padding:3px;">';
            html += '				<input type="text" name="addopt[opt][' + rowindex + '][]" style="width:270px;"> 선택시';
            html += '				판매금액에 <input type="text" name="addopt[addprice][' + rowindex + '][]" size=9 class="ig"> 원 추가';
            html += '			</div>';
            html += '		</td>';

            html += '		<td class="bt1 bb1 bl1 br1">';
            html += '			<div class="checks_0 s_11 ta_center">';
            html += '				<input type="checkbox" name="addoptreq[' + rowindex + ']" value="o" id="ck_17_' + rowindex + '">';
            html += '				<label for="ck_17_' + rowindex + '"></label>';
            html += '			</div>';
            html += '		</td>';
            html += '</tr>';

            jQuery(html).insertBefore(jQuery("#tbAdd").parents('table').find('tbody > tr:last'));
        }

        function del_addopt() {
            var len = jQuery("#tbAdd").parents('table').find('tbody > tr').length;
            if (len > 2) {
                jQuery("#tbAdd").parents('table').find('tbody > tr').eq(len - 2).remove();
            }
        }

        function add_subadd(obj) {
            var idx = jQuery(obj).parents('tr').index();

            var html = '<div style="padding:3px;">';
            html += '		<input type="hidden" name="addopt[sno][' + idx + '][]">';
            html += '		<input type="text" name="addopt[opt][' + idx + '][]" style="width:270px;"> 선택시';
            html += '		판매금액에 <input type="text" name="addopt[addprice][' + idx + '][]"  size=9 class="ig"> 원 추가';
            html += '	</div>';

            jQuery(obj).parents('td').next().append(html);
        }

        function del_subadd(obj) {
            var idx = obj.parentNode.parentNode.rowIndex - 1;
            obj = obj.parentNode.parentNode.childNodes(1).getElementsByTagName('table')[0];
            obj.deleteRow();
        }

        /*** 관련상품 ***/
        function open_box(name, isopen) {
            var mode;
            var isopen = (isopen || document.getElementById('obj_' + name).style.display != "block") ? true : false;
            mode = (isopen) ? "block" : "none";
            document.getElementById('obj_' + name).style.display = document.getElementById('obj2_' + name).style.display = mode;
        }

        function list_goods(name) {
            var category = '';
            open_box(name, true);
            var els = document.forms[0][name + '[]'];
            for (i = 0; i < els.length; i++) if (els[i].value) category = els[i].value;
            var ifrm = eval("ifrm_" + name);
            var goodsnm = eval("document.forms[0].search_" + name + ".value");
            ifrm.location.href = "_goodslist.php?name=" + name + "&category=" + category + "&goodsnm=" + goodsnm;
        }

        function go_list_goods(name) {
            if (event.keyCode == 13) {
                list_goods(name);
                return false;
            }
        }

        function view_goods(name) {
            open_box(name, false);
        }

        function moveEvent(obj, name) {
            obj.onclick = function () {
                spoit(name, this);
            }
            obj.ondblclick = function () {
                remove(name, this);
            }
        }

        function remove(name, obj) {
            var tb = document.getElementById('tb_' + name);
            tb.deleteRow(obj.rowIndex);
            react_goods(name);
        }

        function react_goods(name) {
            var tmp = new Array();
            var obj = document.getElementById('tb_' + name);
            for (i = 0; i < obj.rows.length; i++) {
                tmp[tmp.length] = "<div style='float:left;width:0;border:1 solid #cccccc;margin:1px;' title='" + obj.rows[i].cells[1].getElementsByTagName('div')[0].innerText + "'>" + obj.rows[i].cells[0].innerHTML + "</div>";
            }
            document.getElementById(name + 'X').innerHTML = tmp.join("") + "<div style='clear:both'>";
        }

        var iciRow, preRow, nameObj;

        function spoit(name, obj) {
            nameObj = name;
            iciRow = obj;
            iciHighlight();
        }

        function iciHighlight() {
            if (preRow) preRow.style.backgroundColor = "";
            iciRow.style.backgroundColor = "#FFF4E6";
            preRow = iciRow;
        }

        function moveTree(idx) {
            if (document.getElementById("obj_" + nameObj).style.display != "block") return;
            var objTop = iciRow.parentNode.parentNode;
            var nextPos = iciRow.rowIndex + idx;
            if (nextPos == objTop.rows.length) nextPos = 0;
            objTop.moveRow(iciRow.rowIndex, nextPos);
            react_goods(nameObj);
        }

        function keydnTree(e) {
            if (iciRow == null) return;
            e = e ? e : event;
            switch (e.keyCode) {
                case 38:
                    moveTree(-1);
                    return false;
                case 40:
                    moveTree(1);
                    return false;
            }
        }

        document.onkeydown = keydnTree;

        /*** 상품 이미지 ***/
        function preview(obj) {
            var img = new Image();
            img.onload = function () {
                if (this.width < 600 || this.height < 600) {
                    alert("최소 600X600 픽셀 이상의\n정사각형 이미지를 등록해주세요.\n(권장 : 700x700픽셀)");
                    obj.value = '';
                    return;
                } else {
                    var img = "<div class='checks_0 ch_box s_12 in_b'><label for='s5_20'><span class='f_12 f_bold'>&nbsp;</span></label><img src='" + obj.value + "' alt='상품이미지' class='img_ss'></div>";

                    if (jQuery(obj).parents('tr').children().find('.checks_0')) {
                        jQuery(obj).parents('tr').children().find('.checks_0').remove();
                    }

                    if (obj.value != '') {
                        jQuery(img).insertAfter(jQuery(obj).next());
                    }
                }
            };
            img.src = obj.value;

        }

        function addfld(obj) {
            var k = jQuery(obj).parents('tr').data('k');
            var gubun = jQuery(obj).parents('tr').data('gubun');
            var id = '';

            var html = '';
            var forlabel = Math.round(new Date() / 1000) + '' + Math.ceil(Math.random() * 1000);
            switch (gubun) {
                case 'file':
                    id = "#tb_" + k;

                    html += '<tr data-k="' + k + '" data-gubun="file" class="com_data_1">';
                    html += '	<td class="bt1 bb1 bl1 br1">';
                    html += '		<div class="imgb_1 po_relative">';
                    html += '			<div class="filebox image_box in_b">';
                    html += '				<label class="file-label" for="' + forlabel + '">파일 선택</label>';
                    html += '				<input type="file" name="img_l[]" id="' + forlabel + '" class="upload-hidden" multiple>';
                    html += '				<input type="text" class="upload-name" value="선택된 파일 없음" disabled="disabled">';
                    html += '			</div>';
                    html += '			<button type="button" onclick="delfld(this)" class="op_bt op_bt_d">x 삭제</button>';
                    html += '		</div>';
                    html += '	</td>';
                    html += '</tr>';
                    break;
                case 'url':
                    id = "#tbl_" + k;

                    html += '<tr class="com_data_1" data-k="' + k + '" data-gubun="url">';
                    html += '	<td class="bt1 bb1 bl1 br1">';
                    html += '		<div class="imgb_1 po_relative">';
                    html += '			<input type="text" name="url_l[]" value="" onchange="preview(this)" class="w300 ta_left">';
                    html += '			<button type="button" onclick="delfld(this)" class="op_bt op_bt_d">x 삭제</button>';
                    html += '		</div>';
                    html += '	</td>';
                    html += '</tr>';
                    break;
            }

            if ("file" == gubun) {
                if (jQuery("input[name='img_l[]']").length > 4) {
                    alert("상품 이미지는 최대 5개까지만 등록 가능합니다.");
                    return;
                }
            } else if ("url" == gubun) {
                if (jQuery("input[name='url_l[]']").length > 4) {
                    alert("상품 이미지는 최대 5개까지만 등록 가능합니다.");
                    return;
                }
            }

            //jQuery(html).insertAfter(jQuery(id)); 추가버튼 클릭시 리스트에 마지막에 추가되도록 수정
            if ("file" == gubun) {
                jQuery("#image_attach_method_upload_wrap tbody").append(html);
            } else {
                jQuery("#image_attach_method_link_wrap tbody").append(html);

            }

            var rowspan = jQuery(id).find("th").attr("rowspan") || 1;
            jQuery(id).find("th").attr("rowspan", (Number(rowspan) + 1));

            switch (gubun) {
                case 'file':
                    init_file();
                    break;
            }
        }

        function delfld(obj) {
            var k = jQuery(obj).parents('tr').data('k');
            var gubun = jQuery(obj).parents('tr').data('gubun');
            var id = '';
            switch (gubun) {
                case 'file':
                    id = "#tb_" + k;
                    break;
                case 'url':
                    id = "#tbl_" + k;
                    break;
            }

            var file_input_id = jQuery(obj).siblings('.filebox').find('input[type="file"][name^="img_l"]').attr('id');

            jQuery('#image_attach_method_upload_wrap [type="file"]').filter(function (index, item) {
                if (jQuery(item).attr('id') == file_input_id && attach_imgs[index]) {
                    delete attach_imgs[index];
                }
            });

            jQuery(obj).parents('tr').remove();

            var rowspan = jQuery(id).find("th").attr("rowspan") || 1;
            jQuery(id).find("th").attr("rowspan", (Number(rowspan) - 1));
        }

        function getImglTotalFileSize () {
            var imgs = jQuery('.set_img .img_load img');

            var totalFileSize = 0;

            imgs.each(function () {
                totalFileSize += jQuery(this).attr('fs')*1;
            });

            return totalFileSize;
        }

        function selectUploadImg (obj, index) {
            var file = obj.files[0];
            var reader = new FileReader();
            var maxFileSize = 1024 * 1024 * 2;
            var totMaxFileSize = 20971520;

            if (typeof file == 'undefined') {
                deleteUploadImg(obj, index);
                return false;
            }

            var totalFileSize = getImglTotalFileSize();

            if (file.size > maxFileSize) {
                alert('파일 사이즈가 너무 큽니다.');
                return false;

            } else if (totalFileSize + file.size > totMaxFileSize) {
                alert('이미지파일의 용량은 모두 합해서 '+ '20M' +'B까지만 등록할 수 있습니다.');
                return false;

            } else if (!/gif|jpg|jpeg|png/i.test(file.type)) {
                alert('이미지확장자는 [jpg, gif, png]만 등록이 가능합니다.');
                return false;
            }

            reader.onload = function (e) {
                var img = new Image();
                img.src = e.target.result;

                img.onload = function (f) {
                    if (img.width < 600 || img.height < 600) {
                        alert("최소 600X600 픽셀 이상의 정사각형 이미지를 등록해주세요.\n(권장 : 700x700픽셀)");

                    } else {
                        if (img.width > 700 || img.height > 700) {
                            alert("이미지가 700*700 픽셀의 이미지가 아닐 경우 자동으로 리사이즈 됩니다.\n(현재사이즈 : " + img.width + "x" + img.height + "픽셀)");
                        }

                        jQuery(obj).siblings('.img_load').empty().append('<img src="'+e.target.result+'" fs="'+file.size+'" />');
                        jQuery(obj).siblings('#img_h_l_'+index).val('');
                        jQuery(obj).parent('.tumnail_wrap').addClass('set_img');
                    }
                };
            };
            reader.readAsDataURL(file);
        }

        function deleteUploadImg (obj, index) {
            jQuery(obj).parent('.tumnail_wrap').removeClass('set_img');
            jQuery(obj).siblings('.img_load').empty();
            jQuery(obj).siblings('#img_h_l_'+index).val('');
            jQuery(obj).siblings('#img_f_l_'+index).val('');
        }

        /*** 자동으로 가격필드에 입력값 저장 ***/
        function autoPrice(obj) {
            var name = obj.name;
            var el = $$('input[name^=option_' + name + ']');
            el.each(function (e) {
                e.value = obj.value;
            })
        }

        function vOption() {
            var pao = document.getElementsByName('purchaseApplyOption');

            // 재고량, 사입처, 공급가, 입고량, 사입일 모두 사용 가능하도록
            disabledStyle(document.fm.totstock, "f");
            disabledStyle(document.fm.supply, "f");

            document.fm.totstock.disabled = !document.fm.totstock.disabled;
            openLayer1('objOption', '', 'class');
        }

        function disabledStyle(obj, st) {
            switch (st) {
                case "t" :
                    obj.disabled = true;
                    obj.style.background = "#EEEEEE";
                    break;
                case "f" :
                    obj.disabled = false;
                    obj.style.background = "#FFFFFF";
                    break;
            }
        }

        function chkPurchaseOption(val) {
            var ar_stock = document.getElementsByName('option[stock][]');

            if (val == "1") { // 사입처 동일 적용
                disabledStyle(document.fm.totstock, "t"); // 재고량 X
                disabledStyle(document.fm.pchs_stock, "t"); // 입고량 X
                disabledStyle(document.fm.pchsno, "f"); // 사입처 O
                disabledStyle(document.fm.pchs_pchsdt, "f"); // 사입일 O
            } else { // 사입처 개별 적용
                disabledStyle(document.fm.totstock, "t"); // 재고량 X
                disabledStyle(document.fm.pchsno, "t"); // 사입처 X
                disabledStyle(document.fm.pchs_stock, "t"); // 입고량 X
                disabledStyle(document.fm.pchs_pchsdt, "t"); // 사입일 X
            }

            for (i = 0; i < ar_stock.length; i++) {
                if (val == "1") {
                    ar_stock[i].value = "재고수량 입력 (예. 99)";
                    ar_stock[i].disabled = false;
                } else {
                    ar_stock[i].value = "등록 후 재고 입력";
                    ar_stock[i].disabled = true;
                }
            }
        }

        function chkOptName(obj) {
            if (obj.value == '옵션명2' || obj.value == '옵션명1') {
                obj.className = 'fldtitle';
                obj.value = '';
            }
            if (obj.value == '재고수량 입력 (예. 99)') {
                obj.className = 'opt';
                obj.value = '';
            }
        }

        function chkOptNameOver(obj) {
            if (obj.value == '') {
                obj.className = 'opt gray';
                if (obj.name == 'opt1[]') obj.value = '옵션명1';
                if (obj.name == 'opt2[]') obj.value = '옵션명2';
                if (obj.name == 'option[stock][]') obj.value = '재고수량 입력 (예. 99)';
            }
        }

        function chkDeliveryOption () {
            var order_ct         = jQuery('input[name="order_ct"]').is(':checked');
            var delivery_days    = jQuery('input[name="delivery_days"]').val();

            var order_rt         = jQuery('input[name="order_rt"]').is(':checked');
            var delivery_rt_days = jQuery('input[name="delivery_rt_days"]').val();

            var order_at         = jQuery('input[name="order_at"]').is(':checked');
            var delivery_at_days = jQuery('input[name="delivery_at_days"]').val();

            var result = '';

            if (order_ct && !delivery_days.length) {
                jQuery('input[name="delivery_days"]').focus();
                result = '주문제작';
            }

            if (order_rt && !delivery_rt_days.length) {
                jQuery('input[name="delivery_rt_days"]').focus();
                result = '예약발송';
            }

            if (order_at && !delivery_at_days.length) {
                jQuery('input[name="delivery_at_days"]').focus();
                result = '해외배송';
            }

            return result;
        }

        function chkOption() {
            var obj = document.getElementsByName('opt1[]');
            var chk = false;

            if (obj.length >= 1 && jQuery('#optnm1').val() == '') {
                alert('옵션명1 을 입력해 주세요.');
                jQuery('#optnm1').focus();
                return false;
            }

            for (var i = 0; i < obj.length; i++) {
                chkOptName(obj[i]);
                if ((obj[i].value == '' || obj[i].value == '옵션명1') && obj.length >= 1) {
                    alert('옵션 1은 필수 항목입니다.');
                    obj[i].focus();
                    return false;
                }
                if ((obj[i].value || obj.length > 1) && !chk) chk = true;
            }

            var obj = document.getElementsByName('opt2[]');
            if (obj.length > 1 && jQuery('#optnm2').val() == '') {
                alert('옵션명2를 입력해 주세요.');
                jQuery('#optnm2').focus();
                return false;
            }

            var obj = document.getElementsByName('option[stock][]');
            for (var i = 0; i < obj.length; i++) {
                chkOptName(obj[i]);
            }

            // 소비자정상가 < 쇼핑몰판매가 체크(2018.12.21, 김기완)
            var objConsumer = document.getElementsByName('option[consumer][]');
            var objPrice = document.getElementsByName('option[price][]');
            var halfPrice = '' * 0.5;
            for (var i = 0, cnt = objConsumer.length; i < cnt; i++) {
                if (
                    shAdminGoodsForm.option.provider_extra_view_yn.price_view == 'y' &&
                    shAdminGoodsForm.option.provider_extra_view_yn.consumer_view == 'y'
                ) {
                    if (parseInt(objConsumer[i].value) < parseInt(objPrice[i].value)) {
                        alert("쇼핑몰판매가는 소비자정상가와 같거나 작아야 합니다.");
                        return false;
                    }
                }

                // 기존 쇼핑몰판매가보다 50% 이하인 경우 안내 경고 노출(2019.01.24, 김기완)
                if (halfPrice && objPrice[i].value && halfPrice >= objPrice[i].value) {
                    if (!confirm('변경하시는 쇼핑몰판매가가 기존 쇼핑몰판매가보다 50% 낮은 가격으로 적용됩니다.\n\n진행하시겠습니까?')) {
                        return false;
                    }
                }
            }

            return true;
        }

        function tabLongdescShow(btnObj) {
            var btnId = btnObj.getAttribute('id');
            var btnObj_normal = document.getElementById('btn_longdesc_normal');
            var btnObj_mobile = document.getElementById('btn_longdesc_mobile');
            if (btnId == 'btn_longdesc_normal') {
                document.getElementById('ta_longdesc').style.display = 'block';
                document.getElementById('ta_mlongdesc').style.display = 'none';
                btnObj_normal.style.backgroundColor = '#999';
                btnObj_normal.style.color = '#fff';
                btnObj_mobile.style.backgroundColor = '#f0f0f0';
                btnObj_mobile.style.color = '#000';
            } else if (btnId == 'btn_longdesc_mobile') {
                document.getElementById('ta_longdesc').style.display = 'none';
                document.getElementById('ta_mlongdesc').style.display = 'block';
                btnObj_normal.style.backgroundColor = '#f0f0f0';
                btnObj_normal.style.color = '#000';
                btnObj_mobile.style.backgroundColor = '#999';
                btnObj_mobile.style.color = '#fff';
            }
        }

        function chkTitle() {
            var obj = document.getElementsByName('title[]');
            for (var i = 0; i < obj.length; i++) {
                for (var j = 0; j < obj.length; j++) {
                    if (i != j && obj[i].value == obj[j].value && obj[i].value && obj[j].value) {
                        return false;
                    }
                }
            }
            return true;
        }

        function chkSchPchs() {
            if (document.fm.pchsno.disabled == false) {
                window.open('../goods/popup.purchase_find.php', 'purchaseSearchPop', 'width=640,height=450');
            }
        }


        // 문자열의 byte를 체크 -- 151214 이지연
        function getByte(contents) {
            var str_character;
            var int_char_count;
            var int_contents_length;
            int_char_count = 0;
            int_contents_length = contents.length;

            // 한글과 영문 바이트 계산
            for (k = 0; k < int_contents_length; k++) {
                str_character = contents.charAt(k);
                if (escape(str_character).length > 4)
                    int_char_count += 2;
                else
                    int_char_count++;
            }
            // --한글과 영문 바이트 계산
            return int_char_count;
        }

        // 상품명 55byte 제한 flag - 1 : 50이하 / 2 : 50이상
        var byteFlag = 1;

        // 상품명 100byte 제한 -- 151214 이지연
        // 상품명 55byte 제한 -- 200424 황현범
        function checkByte(obj) {
            var sellerPlan = jQuery("#sellerPlan").val();
            var t = getByte(jQuery('input[name=goodsnm]').val());

            if ( sellerPlan != "L" ) {
                if (t > 55) {
                    jQuery('.chkByte').css('color', 'red');
                    byteFlag = 2;
                } else {
                    jQuery('.chkByte').css('color', 'black');
                    byteFlag = 1;
                }
                jQuery('.chkByte').text(t);
            }

            return t;
        }

        function subStringByByte(str, maxByte) {
            var byte = 0;
            var i    = 0;

            for (i; i < str.length; ++i) {
                var c = str.charCodeAt(i);

                byte += c >> 7 ? 2 : 1;

                if (byte > maxByte) break;
            }

            return str.substring(0, i);
        }

        function chk_delivery_type() {
            var obj = document.getElementsByName('delivery_type');

            if (obj[2].checked == true) document.getElementById('gdi4').style.display = "inline";
            else document.getElementById('gdi4').style.display = "none";

            if (obj[3].checked == true) document.getElementById('gdi5').style.display = "inline";
            else document.getElementById('gdi5').style.display = "none";

            if (obj[4].checked == true) document.getElementById('gdi2').style.display = "inline";
            else document.getElementById('gdi2').style.display = "none";
        }

        function openLayer1(obj, mode, gubun) {
            if (gubun) {
                switch (gubun) {
                    case 'id':
                        obj = _ID(obj);
                        break;
                    case 'class':
                        obj = "." + obj;
                        break;
                }
            } else {
                obj = _ID(obj);
            }


            var flag = jQuery(obj).css('display') != "none" ? "none" : "block";

            if (mode) flag = mode;

            switch (flag) {
                case 'none':
                    jQuery(obj).hide();
                    break;
                case 'block':
                    jQuery(obj).show();
                    break
            }
        }

        function autoStock(tbody, target, isDelete) {
            var stock  = 0;

            if (target && target.rowIndex && isDelete) {
                tbody.find('tr:not(:nth-child('+target.rowIndex+')) input[name^="option_stock"]').each(function () {
                    stock += jQuery(this).val()*1;
                });
            } else {
                tbody.find('tr input[name^="option_stock"]').each(function () {
                    stock += jQuery(this).val()*1;
                });
            }

            jQuery('#goods_price_table input[name="totstock"]').val(stock);

            return stock;
        }

        function init_file() {
            $fileBox = jQuery('#image_attach_method_upload_wrap .filebox');
            fileLoad();
        }

        var _URL = window.URL || window.webkitURL;

        var attach_imgs = [];

        function fileLoad() {
            jQuery.each($fileBox, function (idx) {

                var $this = $fileBox.eq(idx), $btnUpload = $this.find('[type="file"]');

                $btnUpload.off("change").on('change', function () {
                    if (chckFileLen() > 5) {
                        alert("썸네일 이미지는 최대 5개 까지 등록가능합니다.");
                        $btnUpload.val("");
                        return;
                    }

                    let len = this.files.length;
                    let f = "";
                    let fullName = [];

                    for (var i = 0; i < len; i++) {
                        let _this = jQuery(this);
                        let img = new Image();
                        f = this.files[i];

                        img.onload = function (f) {
                            if (img.width < 600 || img.height < 600) {
                                alert("최소 600X600 픽셀 이상의\n정사각형 이미지를 등록해주세요.\n(권장 : 700x700픽셀)");
                                _this.siblings(".upload-name").val("");
                                _this.val("");
                            }
                        };

                        img.src = _URL.createObjectURL(f);

                        if (_this.attr('name') == 'img_l[]') {
                            jQuery('#image_attach_method_upload_wrap [type="file"]').filter(function (index, item) {
                                if (jQuery(item).attr('id') == _this.attr('id')) {
                                    attach_imgs[index] = img.src;
                                }
                            });
                        }
                    }

                    if (len == 1) {
                        fullName = f.name;
                    } else {
                        fullName = "파일 " + len + "개";
                    }

                    jQuery(this).siblings(".upload-name").val(fullName);

                })
            });
        }

        function chckFileLen() {
            //max 5
            let fileLen = 0;

            jQuery.each($fileBox.find('[type=file]'), function (){
                fileLen += this.files.length;
            });

            return fileLen;
        }

        // 브랜드 선택 시 상품명 말머리 추가(2018.11.01, 김기완)
        function addBrandName2GoodsName(brand) {
            $brandno = brand.options[brand.selectedIndex].value;
            if ($brandno) {
                $brandnm = brand.options[brand.selectedIndex].innerText;
                $goodsnm = jQuery("input[name=goodsnm]");

                $goodsnm.val("[" + $brandnm + "]" + $goodsnm.val());
            }
        }

        function checkOptionLength(frm) {
            var flag = true;

            jQuery('input[name="opt1[]"]').each(function () {
                // console.log(jQuery(this).val());
                if (jQuery(this).val().length > 50) {
                    flag = false;
                    alert("옵션값은 50byte를 넘길수 없습니다.");
                    jQuery(this).focus();
                    return ;
                }
            });

            if (!flag){
                return false;
            }
            if (jQuery("#optnm1").val().length > 50) {
                alert("옵션명은 50byte를 넘길수 없습니다.");
                jQuery("#optnm1").focus();
                return false;
            }

            if (jQuery("#optnm2").val().length > 50) {
                alert("옵션명은 50byte를 넘길수 없습니다.");
                jQuery("#optnm2").focus();
                return false;
            }
        }

        // kc 인증필 : 보증서 없음 체크시 인증기관에 해당없음 / 있음 체크시 인증기관 비워지도록 자동 표시 -- 160609 이지연
        function chk_warranty(isChk) {
            document.getElementById("gosi_warranty_organ").value = (isChk ? '' : '해당없음');
        }

        function chk_origin(val) {
            if (val == '대한민국') {
                document.getElementById('gosi_importer').value = '해당없음';
            } else {
                document.getElementById('gosi_importer').value = document.getElementById('gosi_maker').value;
                //document.getElementById('importer').value = brandnm;
            }
        }

        // 준수판매가
        function checkAbidePrice() {
            var price = document.getElementsByName('price')[0];
            var abidePrice = document.getElementsByName('abide_price')[0];

            if (price.value == "" || price.value == undefined) {
                alert('쇼핑몰 판매가를 입력해주세요.');
                price.focus();
                return false;
            }
            if (abidePrice.value == "" || abidePrice.value == undefined) {
                alert('준수 판매가를 입력해주세요.');
                abidePrice.focus();
                return false;
            }
            if (parseInt(abidePrice.value) > parseInt(price.value)) {
                alert('쇼핑몰 판매가는 준수 판매가보다 높아야합니다.');
                price.focus();
                return false;
            }

            return true;
        }

        function chkImg() {
            var type   = jQuery('input[name="image_attach_method"]:checked').val();
            var result = true;

            if (type == 'file') {
                var imgList = jQuery('input[name="img_l[]"]').filter(function () { return this.value; }).toArray();

                if (!imgList.length) {
                    result = false;
                }

            } else if (type == 'url') {
                var urlList = jQuery('input[name="url_l[]"]').filter(function () { return this.value; }).toArray();

                if (!urlList.length) {
                    result = false;
                }
            }

            return result;
        }

    </script>

