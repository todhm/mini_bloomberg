<script type="text/javascript">

var all_popup = [];
jQuery(function(){

    jQuery(".hideBtn").click(function(){

        var seq = jQuery(this).data("seq");
        var type = jQuery(this).data("type");
        var days = new Date();
        
        if(type == '0') days.setDate(days.getDate() + 100000); 
        else days.setDate(days.getDate() + 1);

        jQuery("#popup_"+seq).remove();
 
        setCookie('todayPopup_' + seq, 'done', days);

        showPopupInit();
    });

    jQuery(".btn_close").click(function(){
        jQuery(".layer_provider ").hide();
    });


    jQuery(".showContentBtn").click(function(){
        var seq = jQuery(this).data("seq");
        console.log(seq);
        jQuery(".showContentBtn").removeClass("on");
        jQuery(".content_body").hide();
        jQuery(this).addClass("on");
        jQuery("#content_"+seq).show();
    });

});

// popup-cookie
jQuery(function(){

    var popup_index = 0;
    var popupSeqs = 1;
    console.log(popupSeqs);

            all_popup.push("todayPopup_91");
        var status = getCookie("todayPopup_91");
        if(status == "done"){
            jQuery("#popup_91").remove();
        } else {
            if(popup_index==0) popup_index = 91;
            jQuery("#popup_91").show();
            jQuery(".layer_provider").show();
        }
    
    if(popup_index>0){
        jQuery("#content_"+popup_index).show();
        jQuery(".layer_provider").show();
    }

    showPopupInit();
});


function showPopupInit(){

    var pairs = document.cookie.split(";");
    var cookies = {};

    var hide_popup_list = [];
    for (var i=0; i<pairs.length; i++){
        var pair = pairs[i].split("=");

        var nm = (pair[0]+'').trim();

        if(nm.indexOf("todayPopup_")==0) {
            console.log(nm);
            hide_popup_list.push(nm);
        }
    }

    for (var i = 0; i<hide_popup_list.length; i++) {
        var arrlen = all_popup.length;
        for (var j = 0; j<arrlen; j++) {
            if (hide_popup_list[i] == all_popup[j]) {
                all_popup = all_popup.slice(0, j).concat(all_popup.slice(j+1, arrlen));
            }
        }
    }
    if(all_popup.length == 0){
        jQuery(".layer_comm").hide();
    } else{
        var show_popup_first = all_popup[0];
        var seq = show_popup_first.split("_");
        jQuery("#content_"+seq[1]).show();
        jQuery("#popup_tab_"+seq[1]).addClass("on");
    }
    
}


function select_event_agree(obj,key){
    jQuery(".btn_"+key).parent().css("color","#666");
    jQuery("#"+obj.getAttribute("id")).parent().css("color","#2c9eff");
}

function agree_notice(psno, agree_no, agreeYn) {
    let json_arr = "";

    var arr = []    
    var f = jQuery("input[name^=agree_items_"+agree_no+"]:checked").each(function(i, obj){
        arr.push(jQuery(obj).val())
    });
    json_arr = JSON.stringify(arr);

    var agree = jQuery(this).data("type");
    if(agree == 'popup_agree'){
        if(arr.length == 0){
            var f = jQuery("#popup_chk_"+agree_no).is(":checked");
            if(!f){
                alert('공지 내용 확인 여부에 체크 해주세요.');
                return;
            }
        }
    }
    ifrmHidden.location.href = '/shop/admin_provider/indb.agree.notice.php?psno=' + psno + '&agree_no=' + agree_no + '&agreeYn=' + agreeYn + '&agreeItems=' + json_arr;


}
</script>
